{
  "name": "kerma",
  "version": "1.0.0",
  "description": "This is a Crypto Wallet Website named Kerma",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "start": "npm run lite",
    "lite": "lite-server",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/An-Be/Kerma---Crypto-Wallet.git"
  },
  "author": "Andrea Berrocal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/An-Be/Kerma---Crypto-Wallet/issues"
  },
  "homepage": "https://github.com/An-Be/Kerma---Crypto-Wallet#readme",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "nth": "^1.1.0",
    "nth-check": "^2.0.1",
    "popper.js": "^1.16.1",
    "semver-regex": "^4.0.2",
    "trim-newlines": "^4.0.2"
  }
}
